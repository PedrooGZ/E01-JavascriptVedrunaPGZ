//6. Crea una variable para cada operador aritmetico y muestra su resultado en la consola.
let suma = 2+2;
console.log(suma);
let resta = 2-2;
console.log(resta);
let multi = 2*2;
console.log(multi);
let div = 2/2;
console.log(div);
let mod = 2%2;
console.log(mod)
let decre = 2;
console.log(--decre)
let incre = 2;
console.log(++decre)
console.log("------------------");
